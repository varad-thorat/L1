//Convert to Permutation (Hard Coded)
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        int [] array = new int [] {0,5,5,0,0};
        int res=0;
        Arrays.sort(array);
        for(int i=0; i<array.length; i++){
            if((i+1)< array[i]){
                res = -1;
                break;
            }else if(i+1 > array[i]){
                res = res + (i+1 - array[i]);
            }
        }
        System.out.println(res);
    }
}
